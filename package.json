{
  "name": "marksy-lite",
  "version": "8.0.2-beta",
  "description": "Convert markdown into react components",
  "keywords": [
    "convert",
    "markdown",
    "react"
  ],
  "homepage": "https://github.com/storybooks/marksy",
  "bugs": {
    "url": "https://github.com/storybooks/marksy/issues"
  },
  "license": "MIT",
  "author": "Christian Alfoni",
  "main": "lib/index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybooks/marksy"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src/ --out-dir=lib/ -s",
    "lint": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.json ./src ./app",
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "release-github": "release-it --no-npm  --preRelease=beta",
    "size": "size-limit",
    "start": "webpack-dev-server --devtool eval-source-map --progress --colors --content-base build",
    "test": "jest"
  },
  "dependencies": {
    "he": "^1.2.0",
    "marked": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^9.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "global": "^4.3.2",
    "highlight.js": "^9.15.6",
    "inferno-create-element": "^7.1.12",
    "inferno-server": "^7.1.12",
    "jest": "^24.8.0",
    "json-loader": "^0.5.7",
    "node-libs-browser": "^2.2.0",
    "preact": "^10.0.3",
    "preact-render-to-string": "^5.1.0",
    "prettier": "^1.17.1",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": ">=16 <18",
    "react-dom": ">=16 <18",
    "react-test-renderer": ">=16 <18",
    "regenerator-runtime": "^0.13.3",
    "release-it": "^14.6.1",
    "size-limit": "^2.1.6",
    "style-loader": "^1.0.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "35 KB"
    }
  ]
}
